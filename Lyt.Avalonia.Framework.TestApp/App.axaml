<Application 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="Lyt.Avalonia.Framework.TestApp.App"
    RequestedThemeVariant="Default"
	>
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
		<!-- "old" Badge -->
		<!-- 
		<StyleInclude Source="avares://Lyt.Avalonia.Controls.Theme/Styles/Styles.axaml"/>
		-->
	</Application.Styles>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Assets/Vectors/SvgResourceDictionary.axaml"/>
				<ResourceInclude Source="/Assets/Vectors/FluentSVGResourceDictionary.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
